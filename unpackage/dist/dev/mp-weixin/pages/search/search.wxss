.content {
  padding-top: 25px;
}
/* 头部公共样式 */
.top-bar {
  position: fixed;
  z-index: 1001;
  top: 0;
  left: 0;
  width: 100%;
  height: 88rpx;
  padding-top: 25px;
  /* background: rgba(255,255,255,0.96);
	border-bottom: 1rpx solid $uni-border-color; */
}
.top-bar .top-bar-left {
    float: left;
    padding-left: 16rpx;
}
.top-bar .top-bar-left image {
      margin-top: 10rpx;
      width: 68rpx;
      height: 68rpx;
      border-radius: 16rpx;
}
.top-bar .top-bar-left .text {
      font-size: 32rpx;
      font-weight: 500;
      color: #272832;
      line-height: 88rpx;
}
.top-bar .top-bar-left .back-img {
      margin-top: 21rpx;
      width: 26rpx;
      height: 46rpx;
}
.top-bar .top-bar-center {
    position: absolute;
    text-align: center;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    margin: auto;
}
.top-bar .top-bar-center .logo {
      padding-top: 19rpx;
      width: 88rpx;
      height: 42rpx;
}
.top-bar .top-bar-right {
    float: right;
    padding-right: 14rpx;
}
.top-bar .top-bar-right .search {
      width: 88rpx;
      height: 88rpx;
      display: inline-block;
}
.top-bar .top-bar-right .add {
      width: 88rpx;
      height: 88rpx;
      padding-right: 14rpx;
      display: inline-block;
}
.top-bar .top-bar-right .text {
      font-size: 32rpx;
      font-weight: 500;
      color: #272832;
      line-height: 88rpx;
      padding-right: 32rpx;
}
.top-bar .top-bar-right image {
      padding: 18rpx 0 0 18rpx;
      width: 52rpx;
      height: 52rpx;
}
.top-bar .top-bar-right .more-img {
      height: 88rpx;
      padding-top: 10rpx;
      padding-right: 16rpx;
}
.top-bar .top-bar-right .more-img image {
        width: 52rpx;
        height: 12rpx;
}
.logo {
  text-align: center;
}
.logo image {
    padding-top: 256rpx;
    width: 194rpx;
    height: 92rpx;
    margin: 0 auto;
}
.top-bar {
  background: rgba(255, 255, 255, 0.96);
  border-bottom: 1rpx solid rgba(39, 40, 50, 0.1);
}
.top-bar .search-div {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    z-index: -1;
    float: left;
    padding: 14rpx 118rpx 14rpx 32rpx;
    box-sizing: border-box;
    background: #eee;
}
.top-bar .search {
    height: 60rpx;
    background: #f3f4f6;
    border-radius: 10rpx;
    padding: 0 60rpx 0 12rpx;
}
.top-bar .search-img {
    position: absolute;
    right: 130rpx;
    top: 22rpx;
    width: 40rpx;
    height: 40rpx;
}
.main {
  padding: 88rpx 16rpx;
}
.main .result {
    padding-top: 16rpx;
}
.main .result .title {
      font-size: 44rpx;
      font-weight: 600;
      color: #272832;
      line-height: 60rpx;
}
.main .result .list {
      width: 100%;
      height: 80rpx;
      padding: 20rpx 0;
}
.main .result .list image {
        float: left;
        width: 80rpx;
        height: 80rpx;
        border-radius: 20rpx;
}
.main .result .names {
      float: left;
      padding-left: 16rpx;
}
.main .result .name {
      font-size: 36rpx;
      font-weight: 400;
      color: #272832;
      line-height: 50rpx;
}
.main .result .email {
      font-size: 24rpx;
      color: #272832;
      line-height: 28rpx;
}
.main .result .right-bt {
      float: right;
      width: 120rpx;
      height: 48rpx;
      border-radius: 24rpx;
      font-size: 24rpx;
      line-height: 48rpx;
      text-align: center;
      margin-top: 16rpx;
}
.main .result .send {
      background: #ffe431;
      color: #272832;
}
.main .result .add {
      background: rgba(74, 170, 255, 0.1);
      color: #5799ff;
}

